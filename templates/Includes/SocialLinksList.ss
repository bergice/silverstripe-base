    <li><a href="$SiteConfig.LinkedInLink" target="_blank" ><span class="icon fa fa-linkedin"></span></a></li>
    <% end_if %>
    <% if SiteConfig.Instagram %>
    <li><a href="$SiteConfig.InstagramLink" target="_blank" ><span class="icon fa fa-instagram"></span></a></li>
    <% end_if %>
    <% if SiteConfig.Pinterest %>
    <li><a href="$SiteConfig.PinterestLink" target="_blank" ><span class="icon fa fa-pinterest"></span></a></li>
