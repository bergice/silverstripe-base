<ul class="$ListClass">
    <% if SiteConfig.Facebook %>
    <li><a href="$SiteConfig.FacebookLink" target="_blank" ><span class="icon fa fa-facebook"></span></a></li>
    <% end_if %>
    <% if SiteConfig.Twitter %>
    <li><a href="$SiteConfig.TwitterLink" target="_blank" ><span class="icon fa fa-twitter"></span></a></li>
    <% end_if %>
    <% if SiteConfig.Youtube %>
    <li><a href="$SiteConfig.YoutubeLink" target="_blank" ><span class="icon fa fa-youtube"></span></a></li>
    <% end_if %>
